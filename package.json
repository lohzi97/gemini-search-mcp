{
  "name": "@lohzi97/gemini-search-mcp",
  "version": "0.2.0",
  "description": "An MCP server that wraps Google Gemini CLI for web search capabilities",
  "type": "module",
  "main": "./dist/index.js",
  "bin": {
    "gemini-search-mcp": "./dist/index.js",
    "gemini-search-mcp-http": "./dist/http.js"
  },
  "exports": {
    "./package.json": "./package.json"
  },
  "files": [
    "dist",
    "templates",
    "prompts",
    "LICENSE",
    "README.md"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsx src/index.ts",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "mcp",
    "model-context-protocol",
    "gemini",
    "research",
    "firecrawl",
    "web-scraping",
    "ai-agent"
